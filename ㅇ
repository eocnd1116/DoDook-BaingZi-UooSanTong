onkeypress(bs,"BackSpace")
onkeypress(enter,"Return")
listen()

if kang>0:
    print(kang)
    while True:
        if kang==1:
            kang=2
            bgcolor("yellow")
        if kang==2:
            kang=1
            bgcolor("orange")
        print("ddd")
        sleep(0.4)
